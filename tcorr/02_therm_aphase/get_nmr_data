#!/bin/sh -efu

device -d db_dd -c "*idn?" > /dev/null

mkdir -p data
while read d1 t1 d2 t2 fpref; do
  [ -n "${d1###*}" ] || continue
  s1=$(date -d "$d1 $t1" +%s)
  s2=$(date -d "$d2 $t2" +%s)
  echo "$s1 - $s2"
#  device -d db_dd -c "get_range sweep_main $s1 $s2" > data/${fpref}_nmr.txt
  wget -q -O - "http://slazav.xyz:8085/get_range?name=sweep_main&t1=$s1&t2=$s2" > data/${fpref}_nmr.txt
done << EOF
2018-06-12 7:04:20  2018-06-12 9:08:53  20180612a
2018-06-15 06:52:28  2018-06-15 08:45:42  20180615a
2018-06-15 15:46:24  2018-06-15 17:23:56  20180615b
2018-06-18 13:10:02  2018-06-18 14:42:26  20180618a
2018-06-18 22:38:39  2018-06-19 01:48:15  20180618b
2018-06-19 07:37:48  2018-06-19 09:42:29  20180619a
2018-06-22 09:40:39  2018-06-22 12:31:41  20180622a
2018-06-22 17:32:06  2018-06-22 19:35:02  20180622b
2018-06-26 09:16:00  2018-06-26 11:07:43  20180626a
2018-07-01 08:07:27  2018-07-01 10:48:46   20180701a
2018-07-01 21:29:34  2018-07-02 00:55:49   20180701b
EOF
